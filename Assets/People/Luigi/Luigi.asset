      m_PersistentCalls:
        m_Calls: []
    output:
    - text: "Que tipo de experi\xEAncia tem com faxina?"
      character: {fileID: 11400000, guid: 5823e6d32bbfdfb46b383d52c7878f1e, type: 2}
    - text: "Estar sujo \xE9 algo comum do meu cotidiano."
